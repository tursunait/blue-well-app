{
  "name": "bluewell-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter web dev",
    "dev:api": "cd apps/api && python -m uvicorn main:app --reload",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prisma:generate": "pnpm --filter web prisma generate",
    "prisma:migrate": "pnpm --filter web prisma migrate dev",
    "prisma:seed": "pnpm --filter web prisma db seed",
    "setup": "pnpm install && pnpm prisma:generate && pnpm prisma:migrate"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}

